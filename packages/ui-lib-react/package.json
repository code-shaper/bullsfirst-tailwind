{
  "name": "@bullsfirst/ui-lib-react",
  "description": "ui-lib-react",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "run-p \"bundle -- --watch\" \"generate-css -- --watch\"",
    "build": "rimraf dist && run-s bundle generate-css",
    "lint": "eslint src/**/*.ts*",
    "test": "jest --coverage",
    "bundle": "tsup src/index.ts --format esm,cjs --dts --external react --inject ./react-import.js",
    "generate-css": "postcss src/styles/tailwind.css -o public/main.css",
    "clean": "rimraf .turbo node_modules dist coverage test-output"
  },
  "devDependencies": {
    "@storybook/react": "^6.5.9",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.13",
    "autoprefixer": "^10.4.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.17.0",
    "eslint-config-custom": "*",
    "jest": "^28.1.1",
    "jest-config-custom": "*",
    "jest-environment-jsdom": "^28.1.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-focus-visible": "^6.0.4",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.1.4",
    "ts-jest": "^28.0.5",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "typescript-config-custom": "*"
  },
  "peerDependencies": {
    "@heroicons/react": "^1.*",
    "clsx": "^1.*",
    "react": "^18.*",
    "react-router-dom": "^6.*"
  }
}
